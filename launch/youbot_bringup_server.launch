<launch>

    <include file="$(find youbot_moveit_config)/launch/move_group.launch"/>

    <include file="$(find youbot_navigation_local)/launch/move_base_local.launch" />
    

<!-- <include file="$(find youbot_navigation_global)/launch/move_base_global.launch" />
  <include file="$(find youbot_navigation_global)/launch/amcl.launch" /> -->

    <!-- <node name="laser_flipper" pkg="laser_flipper" type="laser_flipper" /> -->
    <arg name="scan" value="base_scan"/>
    <node name="slam_gmapping" pkg="gmapping" type="slam_gmapping"
        args="scan:=$(arg scan) _xmin:=-10 _ymin:=-10 _xmax:=10 _ymax:=10" />
    
    <include file="$(find octomap_server)/launch/octomap_mapping.launch"/>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find youbot_srt)/launch/srt_full.rviz" />

<!--     <node name="rviz" pkg="rviz" type="rviz" args="-d $(find youbot_srt)/launch/srt_no_moveit.rviz" /> -->

</launch>
